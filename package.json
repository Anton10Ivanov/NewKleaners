{
  "name": "kleaners",
  "version": "1.1.0",
  "description": "Professional Cleaning Services Platform - Connecting customers with verified cleaning service providers",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:clean": "node scripts/dev/start-dev.js",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest --config config/vitest.config.ts",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test --config config/playwright.config.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "unused": "unimported",
    "unused:fix": "unimported --fix",
    "knip": "knip --config config/knip.json",
    "knip:fix": "knip --fix",
    "deps:check": "npm-check-updates",
    "deps:update": "npm-check-updates -u && npm install",
    "deps:audit": "audit-ci --config config/audit-ci.json",
    "deps:unused": "depcheck",
    "deps:sync": "syncpack fix-mismatches",
    "madge": "madge --circular --extensions ts,tsx src",
    "madge:graph": "madge --image graph.svg --extensions ts,tsx src",
    "cruise": "dependency-cruiser --config .dependency-cruiser.js src",
    "cruise:graph": "dependency-cruiser --config .dependency-cruiser.js --output-type dot src | dot -T svg > dependency-graph.svg",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next out dist build",
    "pre-commit": "npm run standards:validate",
    "prepare": "husky install",
    "setup": "node scripts/setup-dev.js",
    "standards:validate": "npm run type-check && npm run lint && npm run format:check",
    "lint:fix": "eslint . --fix",
    "quality:check": "node scripts/quality-check.js",
    "cleanup:unused": "node scripts/cleanup-unused.js",
    "config:check": "node scripts/config-manager.js check",
    "config:validate": "node scripts/config-manager.js validate",
    "config:summary": "node scripts/config-manager.js summary",
    "mcp:build": "cd mcp-server && npm run build",
    "mcp:start": "cd mcp-server && npm start",
    "mcp:dev": "cd mcp-server && npm run dev",
    "quality:all": "npm run standards:validate && npm run test && npm run deps:audit && npm run unused && npm run knip",
    "quality:ci": "npm run quality:all && npm run test:e2e"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@modelcontextprotocol/sdk": "^1.19.1",
    "@modelcontextprotocol/server-filesystem": "^2025.8.21",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@supabase/supabase-js": "^2.38.4",
    "@tanstack/react-query": "^5.14.2",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "next": "15.5.3",
    "react": "^18.2.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "sonner": "^1.2.4",
    "stripe": "^13.11.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.40.1",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-themes": "^8.6.14",
    "@storybook/nextjs": "^8.6.14",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "audit-ci": "^7.0.1",
    "autoprefixer": "^10.4.16",
    "depcheck": "^1.4.7",
    "dependency-cruiser": "^16.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-config-next": "15.5.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "knip": "^4.0.0",
    "lint-staged": "^15.2.0",
    "madge": "^6.1.0",
    "msw": "^2.0.8",
    "npm-check-updates": "^16.14.15",
    "playwright": "^1.40.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "syncpack": "^12.0.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "unimported": "^1.0.0",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
