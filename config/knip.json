{
  "$schema": "https://unpkg.com/knip@4/schema.json",
  "entry": [
    "app/**/*.{js,ts,jsx,tsx}",
    "pages/**/*.{js,ts,jsx,tsx}",
    "next.config.js",
    "tailwind.config.ts",
    "postcss.config.js",
    "vitest.config.ts",
    "playwright.config.ts"
  ],
  "project": [
    "app/**/*.{js,ts,jsx,tsx}",
    "components/**/*.{js,ts,jsx,tsx}",
    "lib/**/*.{js,ts,jsx,tsx}",
    "hooks/**/*.{js,ts,jsx,tsx}",
    "utils/**/*.{js,ts,jsx,tsx}",
    "services/**/*.{js,ts,jsx,tsx}",
    "schemas/**/*.{js,ts,jsx,tsx}",
    "types/**/*.{js,ts,jsx,tsx}",
    "scripts/**/*.{js,ts,jsx,tsx}",
    "tests/**/*.{js,ts,jsx,tsx}",
    "stories/**/*.{js,ts,jsx,tsx}"
  ],
  "ignore": [
    "**/*.d.ts",
    "**/*.config.*",
    "**/coverage/**",
    "**/dist/**",
    "**/.next/**",
    "**/storybook-static/**",
    "**/node_modules/**",
    "**/public/**"
  ],
  "ignoreDependencies": [
    "@types/jest",
    "@types/node",
    "@types/react",
    "@types/react-dom",
    "typescript",
    "eslint",
    "prettier",
    "tailwindcss",
    "postcss",
    "autoprefixer"
  ],
  "ignoreExportsUsedInFile": true,
  "ignoreBinaries": ["next", "tsc", "eslint", "prettier"],
  "includeEntryExports": true,
  "includeEntryFiles": true,
  "includeEntryTypes": true,
  "includeEntryValues": true,
  "includeEntryEnums": true,
  "includeEntryNamespaces": true,
  "includeEntryInterfaces": true,
  "includeEntryTypeAliases": true,
  "includeEntryClasses": true,
  "includeEntryFunctions": true,
  "includeEntryVariables": true,
  "includeEntryConstants": true,
  "includeEntryModules": true,
  "includeEntryNamespaces": true,
  "includeEntryTypes": true,
  "includeEntryValues": true,
  "includeEntryEnums": true,
  "includeEntryNamespaces": true,
  "includeEntryInterfaces": true,
  "includeEntryTypeAliases": true,
  "includeEntryClasses": true,
  "includeEntryFunctions": true,
  "includeEntryVariables": true,
  "includeEntryConstants": true,
  "includeEntryModules": true
}
